<UserControl x:Class="EricNee.AutoStartDesktop.AppIconUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EricNee.AutoStartDesktop"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Initialized="UserControl_Initialized" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid x:Name="AppIconGrid" Width="100" Height="100">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <!--<Image x:Name="AppImage" Grid.Column="0" Grid.Row="0" Height="48" Width="48"></Image>-->
        <Label x:Name="AppName" Grid.Row="0" Grid.Column="0" MouseLeftButtonUp="AppName_MouseLeftButtonUp" FontSize="20"  />
    </Grid>
</UserControl>
